{
  "name": "eusi-sdk-core",
  "version": "1.0.1",
  "description": "SDK for accessing EUSI delivery API",
  "repository": {
    "type": "git",
    "url": "https://github.com/jsguru-io/eusi-sdk-core-js"
  },
  "keywords": [
    "eusi",
    "sdk",
    "core",
    "CMS",
    "headless",
    "delivery",
    "API"
  ],
  "browser": "dist/browser/eusi-sdk-core.js",
  "main": "dist/cjs/eusi-sdk-core.js",
  "module": "dist/es/eusi-sdk-core.js",
  "author": "JS Guru International",
  "license": "MIT",
  "devDependencies": {
    "babel-eslint": "^8.0.2",
    "babel-plugin-external-helpers": "^6.22.0",
    "babel-preset-env": "^1.6.0",
    "eslint": "^4.10.0",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.6.0",
    "husky": "^0.14.3",
    "jest": "^22.2.1",
    "jest-cli": "^22.1.4",
    "lint-staged": "^5.0.0",
    "rollup": "^0.55.0",
    "rollup-plugin-babel": "^2.7.1",
    "rollup-plugin-commonjs": "^8.0.2",
    "rollup-plugin-node-resolve": "^3.0.0",
    "rollup-plugin-strip": "^1.1.1",
    "rollup-plugin-uglify": "^3.0.0"
  },
  "scripts": {
    "build": "rollup -c",
    "dev": "rollup -c -w",
    "precommit": "lint-staged && npm run build && npm run test && git add --all",
    "test": "jest",
    "test-watch": "jest --watch",
    "test-debug": "node --inspect-brk ./node_modules/jest/bin/jest.js --runInBand "
  },
  "files": [
    "dist",
    "lib"
  ],
  "jest": {
    "clearMocks": true
  },
  "lint-staged": {
    "lib/**/*.js": [
      "eslint --fix"
    ]
  }
}
